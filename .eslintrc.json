{
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [],
    "rules": {
      "prefer-const": "error",
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "error",
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "parser": "flow",
          "endOfLine": "auto"
        }
      ]
    }
  }